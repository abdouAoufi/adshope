doctype html
html(lang="en")
   head
      meta(charset="UTF-8")
      meta(http-equiv="X-UA-Compatible", content="IE=edge")
      meta(name="viewport", content="width=device-width, initial-scale=1.0")
      title #{docTitle}
      link(rel="stylesheet", href="/css/main.css")
   body 
      header.main-header 
         nav.main-header__nav 
            ul.main-header__item-list
               li.main-header__item
                  a.active(href="/") Shop   
               li.main-header__item
                  a(href="/admin/add-product") Add Product  
               li.main-header__item
                  a(href="/order") Order Product     
      main
         if prods.length > 0
            h1.title list of products :
            div  
            .grid
                  each product in prods
                     article.card.product-item 
                        header.card__header 
                           h1.product__title #{product.title}
                        div.card__image 
                           img(src="data:image/jpeg;base64,", alt="")  
                           p name : #{product.title} 
                           p Quantinty : #{product.quantity}     
         else 
            h1 no product                
         
         if ords.length > 0 
            h1.title list of orders :
            div  
            .grid
                  each order in ords
                     article.card.product-item 
                        header.card__header 
                           h1.product__title #{order.title}
                        div.card__image 
                           img(src="https://i.vimeocdn.com/portrait/6462071_640x640", alt="hed", width="200")  
                           p name : #{order.title} 
                           p Quantinty : #{order.quantity}                     
                           p Date : #{order.date}                     
                           p Location : #{order.location}     
         else 
              h1 no orders                                  